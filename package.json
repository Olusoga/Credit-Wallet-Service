{
  "name": "lendsqr-master",
  "version": "1.0.0",
  "description": "## Development",
  "main": "server.ts",
  "scripts": {
    "start": "node ./dist/src/server.js",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec node_modules/.bin/ts-node src/server.ts",
    "migrate": "knex migrate:latest",
    "make": "knex migrate:make",
    "seed": "knex seed:make",
    "build": "npm run clean && tsc",
    "clean": "rmdir /s /q dist || true",
    "format": "prettier --config .prettierrc.js --write  \"**/*.{ts,tsx}\"",
    "format:check": "prettier --config .prettierrc.js --write './**/*.{js,ts,json,yml,yaml}'",
    "lint": "eslint --ignore-path .eslintignore --ext .ts,.js .",
    "lint:fix": "eslint --ignore-path .eslintignore --ext .ts,.js --fix .",
    "test:all": "cross-env NODE_ENV=test jest --verbose --runInBand --collectCoverage=true ./tests",
    "test": "jest",
    "test-integration": "cross-env NODE_ENV=test jest --verbose --runInBand --collectCoverage=true ./tests/integration"
  },
  "author": "Olusoga",
  "license": "ISC",
  "devDependencies": {
    "@nestjs/testing": "^10.3.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.8",
    "@types/nodemailer": "^6.4.15",
    "@types/reflect-metadata": "^0.1.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mysql2": "^3.10.1",
    "nodemailer": "^6.9.14",
    "nodemon": "^3.1.4",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
