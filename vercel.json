{
    "version": 2,
    "public": true,
    "builds": [
      {
        "src": "src/**/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/src/application.ts" },
        { "src": "/swagger", "dest": "/src/swagger-config.ts" },
        {
            "src": "/swagger/(.*)",
            "headers": { "cache-control": "s-maxage=1, stale-while-revalidate" },
            "dest": "/swagger"
          }
      ]
  }